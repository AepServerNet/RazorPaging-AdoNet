@page
@using RazorPagingAdoNet.Models.ViewModels
@model RazorPagingAdoNet.Models.ViewModels.CourseListViewModel

<h1>Utenti registrati:</h1>
<hr>
<ul>
    @foreach (UserViewModel item in Model.Utenti.Results)
    {
        //Qui mostro i campi del database
        <li>Cognome: @item.Cognome</li>
        <li>Nome: @item.Nome</li>
        <li>Email: @item.Email</li>
    }
</ul>